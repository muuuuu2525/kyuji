import sys

CONVERSION_MAP = {
    '學': '学', '國': '国', '體': '体', '舊': '旧', '辨': '弁',
    '瓣': '弁', '辯': '弁', '廣': '広', '應': '応', '櫻': '桜',
    '轉': '転', '氣': '気', '圓': '円', '實': '実', '關': '関',
    '濱': '浜', '權': '権', '變': '変', '戰': '戦', '續': '続'
}

def convert_text(text):
    result = []
    for char in text:
        result.append(CONVERSION_MAP.get(char, char))
    return "".join(result)

def main():
    try:
        input_text = sys.stdin.read()
        converted = convert_text(input_text)
        sys.stdout.write(converted)
    except Exception as e:
        sys.stderr.write(str(e))
        sys.exit(1)

if __name__ == '__main__':
    main()
